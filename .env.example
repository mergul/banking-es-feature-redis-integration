# Example environment variables for local development and testing.
# Copy this file to .env and replace with your actual values where necessary.

# PostgreSQL database URL
# Format: postgresql://[user[:password]@][host][:port][/database_name][?options]
DATABASE_URL=postgresql://postgres:password@postgres:5432/banking_es

# Redis URL
# Used for caching, event batching, and command de-duplication.
# Format: redis://[username:password@]host:port[/db_number]
REDIS_URL=redis://redis:6379
