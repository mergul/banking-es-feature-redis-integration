{
  "name": "banking-es-connector",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.user": "postgres",
    "database.dbname": "banking_es",
    "database.server.name": "banking-es-server",
    "schema.history.internal.kafka.bootstrap.servers": "localhost:9092",
    "database.port": "5432",
    "plugin.name": "pgoutput",
    "topic.prefix": "banking-es",
    "schema.history.internal.kafka.topic": "schema-changes.banking-es",
    "database.hostname": "localhost",
    "database.password": "Francisco1",
    "name": "banking-es-connector",
    "table.include.list": "public.kafka_outbox_cdc",

    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "transforms.unwrap.operation.header": "true",

    "poll.interval.ms": "10",
    "max.queue.size": "8192",
    "max.batch.size": "2048",
    "snapshot.mode": "initial",
    "snapshot.delay.ms": "0",
    "snapshot.fetch.size": "1024",
    "heartbeat.interval.ms": "1000",
    "include.schema.changes": "false",
    "provide.transaction.metadata": "false",
    "tombstones.on.delete": "true",
    "database.history.kafka.recovery.attempts": "3",
    "database.history.kafka.recovery.poll.interval.ms": "100",
    "database.history.store.only.captured.tables.ddl": "true",
    "database.history.skip.unparseable.ddl": "true",
    "transforms.outbox.table.field.event.timestamp": "deleted_at",
    "transforms.outbox.route.tombstone.on.empty.payload": "true"
  }
}
